public class Ex01_BtnEvt extends MyFrame {
	private Button[] btnArr = new Button[2];
	{ // 초기화 블럭 - 필드를 초기화 하는 구역, 메서드에서 실행문쓰기 
		btnArr[0] = new Button("Button1");
		btnArr[1] = new Button("Button2");
	}
	
	public Ex01_BtnEvt() {
		start();
	}

	public void start() {
		
	}

	public static void main(String[] args) {
		
	}

}
