package member.controller;

import static member.resource.R.*;
import member.resource.R;
import member.view.MemberView;

public class DispatcherController extends R{
	private static MemberView view = null;
	private static Controller ctrl = null; // 멤버필드로 선언
	
	
	public static void run(){
//		view = new InputController().hadlerRequest();
//		view = new MenuController().hadlerRequest();
		
		ctrl = ctrlMapper.get(no);
		view = ctrl.hadlerRequest();
		
		mainActivity.setContentView(view);
		
		no = MENU;
	}
}
