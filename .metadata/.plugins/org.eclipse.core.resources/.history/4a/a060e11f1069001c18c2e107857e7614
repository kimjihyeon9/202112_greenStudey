package Ch05_실습문제풀이_2;

public class Won2Dollar extends Converter {
	
	public Won2Dollar(double ratio) {
		super.ratio = ratio;
	}

	@Override
	protected double convert(double src) {
		return src / ratio;
	}

	@Override
	protected String getSrcString() {
		return srcString;
	}

	@Override
	protected String getDestString() {
		return destString;
	}
	
	public 

}
