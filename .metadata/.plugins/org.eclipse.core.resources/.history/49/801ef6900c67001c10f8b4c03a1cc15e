package org.constudy21.saram.view;

import org.constudy21.model.SaramVo;
import org.constudy21.saram.R;

public class SaramDelete extends SaramView {

	@Override
	public void display() {
		if (R.type == R.SEARCH) {
			System.out.println(":::::사람 정보 삭제:::::");
			System.out.print("삭제 할 번호>>");
			int no = scan.nextInt();
			R.saramVo = new SaramVo(no, null, null, null);
			
		}
	}
}
