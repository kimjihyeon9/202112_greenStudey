package Ch07_ex02_menu;

class Menu {
	public void display() {
		System.out.println("1.게시판 2.갤러리 3.주소록 4.종료");
	}
}

abstract class Controller {
	public abstract void run();
}

public class DispatcherEx {
	public DispatcherEx() {
		action();
	}
	private void action() {
		// 메뉴 보여준다.
	}

	public static void main(String[] args) {
		new DispatcherEx();
	}

}
