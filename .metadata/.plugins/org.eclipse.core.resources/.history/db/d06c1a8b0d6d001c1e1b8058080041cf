package Day15_excercise05;

class Student{
	String 이름;
	String 학과;
	String 학번;
	String 평균;
	
	public Student() {
		this("", "", "", "");
	}

	public Student(String 이름, String 학과, String 학번, String 평균) {
		this.이름 = 이름;
		this.학과 = 학과;
		this.학번 = 학번;
		this.평균 = 평균;
	}

	public String get이름() {
		return 이름;
	}

	@Override
	public String toString() {
		return 이름 + "," + 학과 + "," + 학번 + "," + 평균;
	}
	public String toString(int i) {
		return "이름: " + 이름 + "\n학과: " + 학과 + "\n학번: " + 학번 + "\n학점평균: " + 평균;
	}
	
}

public class Ch07Excercise05 {
	public Ch07Excercise05() {
		start();
	}
	
	private void start() {
		// 4명의 학생 정보 입력
	}

	public static void main(String[] args) {
		new Ch07Excercise05();
	}

}
