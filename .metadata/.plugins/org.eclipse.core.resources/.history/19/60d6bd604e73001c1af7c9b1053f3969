package Day20_6;

import javax.swing.JButton;
import javax.swing.JPanel;

public class BottomPane extends JPanel {
	JButton btn1 = new JButton("All");
	JButton btn2 = new JButton("Input");
	JButton btn3 = new JButton("Search");
	JButton btn4 = new JButton("Modify");
	JButton btn5 = new JButton("Delete");
	
	public BottomPane() {
		this.add(btn1);
		this.add(btn2);
		this.add(btn3);
		this.add(btn4);
		this.add(btn5);
	}
}
